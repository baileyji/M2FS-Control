[Unit]
Description=IFU-M Arduino Shield
#After=ifushield.device

[Install]
#WantedBy=director.service
#WantedBy=ifushield.device

[Service]
Type=simple
ExecStart=/M2FS-Control/bin/ifushieldAgent.py
WorkingDirectory=/M2FS-Control/
RestartSec=100ms
Restart=always
KillMode=control-group
KillSignal=SIGTERM
SendSIGKILL=yes