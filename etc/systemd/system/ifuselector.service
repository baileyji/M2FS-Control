[Unit]
Description=IFU-M Selector
Wants=selector.device
After=selector.device

[Install]
WantedBy=director.service

[Service]
Type=simple
ExecStart=/M2FS-Control/bin/selectorAgent.py
WorkingDirectory=/M2FS-Control/
RestartSec=100ms
Restart=always
KillMode=control-group
KillSignal=SIGTERM
SendSIGKILL=yes