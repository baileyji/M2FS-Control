[Unit]
Description=M2FS MCalLED
After=network.target

[Install]
WantedBy=multi-user.target
WantedBy=m2fs_director.service

[Service]
Type=simple
ExecStart=/M2FS-Control/bin/mcalAgent.py
WorkingDirectory=/M2FS-Control/
RestartSec=100ms
Restart=always
KillMode=control-group
KillSignal=SIGTERM
SendSIGKILL=yes